    <script type="text/javascript">
        $(document).ready(function () {
            var controller = $.superscrollorama();
            // set duration, in pixels scrolled, for pinned element
            if ($(".effect-it").size() > 0) {
                $(".effect-it").each(function () {
                    if ($(this).hasClass("fade-it")) {
                        var idfade = "#" + $(this).attr("id");
                        controller.addTween(idfade, TweenMax.from($(idfade), .5, { css: { opacity: 0 } }));
                    } else if ($(this).hasClass("fly-it")) {
                        var idfade = "#" + $(this).attr("id");
                        controller.addTween(idfade, TweenMax.from($(idfade), .25, { css: { right: '100%' }, ease: Quad.easeInOut }));
                    }
                });
            }
        });
